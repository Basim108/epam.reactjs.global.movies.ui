{
  "name": "movies-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --watchAll=false --silent",
    "test:ci": "react-scripts test --coverage --watchAll=false --browsers=ChromeHeadless --silent",
    "eject": "react-scripts eject",
    "wdio": "wdio run ./wdio.conf.js",
    "sb": "start-storybook -p 6006 -s public",
    "build-sb": "build-storybook -s public",
    "lint": "prettier -w .",
    "lintDry": "prettier -c ."
  },
  "jest": {
    "collectCoverageFrom": [
      "!src/{index,App,reportWebVitals}.{js,jsx}",
      "!src/**/*.stories.{js,jsx}",
      "!src/**/*.test.{js,jsx}",
      "!src/**/*.e2e.{js,jsx}"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.11",
    "@mui/material": "^5.11.14",
    "@mui/x-date-pickers": "^6.0.4",
    "dayjs": "^1.11.7",
    "material-ui-popup-state": "^5.0.8",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-portal": "^4.2.2",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/node-logger": "^6.5.16",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/dom": "^9.0.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@wdio/cli": "^8.6.5",
    "@wdio/jasmine-framework": "^8.6.3",
    "@wdio/local-runner": "^8.6.3",
    "@wdio/spec-reporter": "^8.4.0",
    "babel-plugin-named-exports-order": "^0.0.2",
    "chromedriver": "^111.0.0",
    "prettier": "^2.8.7",
    "wdio-chromedriver-service": "^8.1.1",
    "webpack": "^5.76.3"
  }
}
